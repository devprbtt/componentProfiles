    <logical_component logical_component_name="Media_converter">
        <implementation>
            <input name_on_component="Audio Input"/>
            <input name_on_component="VGA Input"/>
            <output name_on_component="HDMI Output"/>
            <output name_on_component="Audio Output"/>
            <output name_on_component="VGA Output"/>
        </implementation>
    </logical_component>
</component>
