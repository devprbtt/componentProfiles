<?xml version="1.0" encoding="UTF-8"?>
<component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="racepoint_component_profile.xsd" manufacturer="Absolute" model="NDS-VGH11D" alias="Absolute Nexos VGA + Audio to HDMI, NDS-VGH11D" device_class="Media_converter" minimum_component_engine_version="0" rpm_xml_version="$Revision: 1.0 $">
    <media_interfaces>
