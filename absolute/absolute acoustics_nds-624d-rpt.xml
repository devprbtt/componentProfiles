<?xml version="1.0" encoding="UTF-8"?>
<component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="racepoint_component_profile.xsd" manufacturer="Absolute" model="NDS-624D-RPT" alias="Absolute Nexos Long-haul HDMI Repeater, NDS-624D-RPT" device_class="Media_converter" minimum_component_engine_version="0" rpm_xml_version="$Revision: 1.2 $">

    <notes>
        This product is a Dual CAT-X HDMI repeater. Compatible with Absolute NDS-621D-TX, NDS-623D-LH-RX, NDS-MX42-H, NDS-MX44-H and NDS-MX88-H devices, for HDMI over CAT-X transmission.
    </notes>

    <component_properties>
        <always_on_allowed>false</always_on_allowed>
    </component_properties>
    
    <media_interfaces>
        <input name_on_component="HDMI UTP In">
            <combined_media>
                <audio_media type="rj45_audio"/>
                <video_media type="rj45"/>
            </combined_media>
            <resource resource_type="AV_AUDIOSWITCH_FUNCTION"/>
            <resource resource_type="AV_VIDEOSWITCH_FUNCTION"/>
        </input>
        <output name_on_component="HDMI Out">
            <combined_media>
                <audio_media type="hdmi_audio"/>
                <video_media type="hdmi"/>
            </combined_media>
        </output>
        <output name_on_component="HDMI UTP Out">
            <combined_media>
                <audio_media type="rj45_audio"/>
                <video_media type="rj45"/>
            </combined_media>
        </output>
    </media_interfaces>
    
    <logical_component logical_component_name="Media_converter">
        <implementation>
            <input name_on_component="HDMI UTP In"/>
            <output name_on_component="HDMI Out"/>
            <output name_on_component="HDMI UTP Out"/>
        </implementation>
    </logical_component>
</component>