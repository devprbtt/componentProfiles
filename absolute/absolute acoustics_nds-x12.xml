<?xml version="1.0" encoding="UTF-8"?>
<component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="racepoint_component_profile.xsd" manufacturer="Absolute" model="NDS-X12"  alias="Absolute Nexos HDMI Converter, NDS-X12" device_class="AV_switch" minimum_component_engine_version="0.0" rpm_xml_version="$Revision: 1.10 $">
    <media_interfaces>
        <input name_on_component="HDMI Input">
            <combined_media>
                <audio_media type="hdmi_audio"/>
                <video_media type="hdmi"/>
            </combined_media>
            <resource resource_type="AV_AUDIOSWITCH_FUNCTION"/>
            <resource resource_type="AV_VIDEOSWITCH_FUNCTION"/>
        </input>
        <output name_on_component="Output Sub (Bypass)">
            <combined_media>
                <audio_media type="hdmi_audio"/>
                <video_media type="hdmi"/>
            </combined_media>
        </output>
        <output name_on_component="Output Main (1080p)">
            <combined_media>
                <audio_media type="hdmi_audio"/>
                <video_media type="hdmi"/>
            </combined_media>
        </output>
        <output name_on_component="RCA Stereo Out">
            <combined_media>
                <audio_media type="rca_stereo"/>
            </combined_media>
        </output>
    </media_interfaces>
    <logical_component logical_component_name="AV_switch">
        <implementation>
            <input name_on_component="HDMI Input"></input>
            <output name_on_component="Output Sub (Bypass)"></output>
            <output name_on_component="RCA Stereo Out"></output>
        </implementation>
    </logical_component>
    <logical_component logical_component_name="AV_switch_2">
        <implementation>
            <input name_on_component="HDMI Input"></input>
            <output name_on_component="Output Main (1080p)"></output>
            <output name_on_component="RCA Stereo Out"></output>
        </implementation>
    </logical_component>
</component>
